{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "./tsconfig.json"
  },
  "monorepo": true,
  "root": "apps",
  "projects": {
    "init-config": {
      "type": "application",
      "root": "apps/config",
      "entryFile": "init",
      "sourceRoot": "apps/config/src",
      "compilerOptions": {
        "tsConfigPath": "apps/config/tsconfig.app.json"
      }
    },
    "config": {
      "type": "application",
      "root": "apps/config",
      "entryFile": "main",
      "sourceRoot": "apps/config/src",
      "compilerOptions": {
        "tsConfigPath": "apps/config/tsconfig.app.json"
      }
    },
    "composer": {
      "type": "application",
      "root": "apps/composer",
      "entryFile": "main",
      "sourceRoot": "apps/composer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/composer/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "captcha": {
      "type": "application",
      "root": "apps/captcha",
      "entryFile": "main",
      "sourceRoot": "apps/captcha/src",
      "compilerOptions": {
        "tsConfigPath": "apps/captcha/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "media": {
      "type": "application",
      "root": "apps/media",
      "entryFile": "main",
      "sourceRoot": "apps/media/src",
      "compilerOptions": {
        "tsConfigPath": "apps/media/tsconfig.app.json"
      }
    },
    "explorer": {
      "type": "application",
      "root": "apps/explorer",
      "entryFile": "main",
      "sourceRoot": "apps/explorer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/explorer/tsconfig.app.json"
      }
    },
    "xml-data-bridge": {
      "type": "application",
      "root": "apps/xml-data-bridge",
      "entryFile": "main",
      "sourceRoot": "apps/xml-data-bridge/src",
      "compilerOptions": {
        "tsConfigPath": "apps/xml-data-bridge/tsconfig.app.json"
      }
    },
    "file": {
      "type": "application",
      "root": "apps/file",
      "entryFile": "main",
      "sourceRoot": "apps/file/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file/tsconfig.app.json"
      }
    },
    "locale": {
      "type": "application",
      "root": "apps/locale",
      "entryFile": "main",
      "sourceRoot": "apps/locale/src",
      "compilerOptions": {
        "tsConfigPath": "apps/locale/tsconfig.app.json"
      }
    }
  }
}
